<pip:Resource xmlns:pip="http://nrg.wustl.edu/pipeline">
	<pip:name>organize_data.sh</pip:name>
	<pip:location>/xnat/pipeline/amc-catalog/QAP/resource</pip:location>
	<pip:type>Executable</pip:type>
	<pip:description>Organize imaging data using BIDS specification, suitable data tree-structure for running QAP applications</pip:description>
	<!--<pip:commandPrefix>...</pip:commandPrefix>-->
	<pip:estimated_time>00:00:30</pip:estimated_time>
	<pip:input>
		<pip:argument id="project" >
			<pip:description>project ID</pip:description>
		</pip:argument>
        <pip:argument id="subjectName" >
			<pip:description>subject label</pip:description>
		</pip:argument>
        <pip:argument id="sessionName" >
			<pip:description>session label</pip:description>
		</pip:argument>
        <pip:argument id="scans_type" >
			<pip:description>type of scans to process QAP for</pip:description>
		</pip:argument>
        <pip:argument id="input_data_directory" >
			<pip:description>location where raw imaging data is stored</pip:description>
		</pip:argument>
	</pip:input>
</pip:Resource>